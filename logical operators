#include <stdio.h>
int main()
{
int a=10,b=10,c=5,result;
result=(a==b)&&(c>a);
printf("(a==b)&&(c>a)=%d",result);
result=(a==b)&&(c<a);
printf("(a==b)&&(c<a)=%d",result);
result=(a==b)||(c>a);
printf("(a==b)||(c>a)=%d",result);
result=(a!=b)||(c<a);
printf("(a!=b)||(c<a)=%d",result);
result=!(a!=b);
printf("(a!=b)=%d",result);
return 0;
}
