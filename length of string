
// Online C compiler to run C program online
#include <stdio.h>
#include<string.h>
int main() {
   char a[20]="program";
   char b[20]={'p','r','o','g','r','a','m','\0'};
   printf("length of string a=%d",strlen(a));
   printf("length of string b=%d",strlen(b));
   return 0;
}


length of string a=7length of string b=7
