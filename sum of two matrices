#include<stdio.h>
int main()
{
    int a[2][2]={1,1,1,1};
    int b[2][2]={1,1,1,1};
    int i,j,sum[i][j];
    for(i=0;i<2;i++)
     {   
         for(j=0;j<2;j++)
            {
                sum[i][j]=a[i][j]+b[i][j];
        
            }
     }
     
      for(i=0;i<2;i++)
         {   
            for(j=0;j<2;j++)
             {
    
                printf("%d\t",sum[i][j]);
                
                
            }
         }
     
    return 0;
    }
    

2	2	2	2	
