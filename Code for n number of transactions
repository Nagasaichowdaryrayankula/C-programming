#include <stdio.h>
int main()
{
    int a,b,c,d,e,f;
    a=8000;
    printf("Initial balance:%d\n",a);
    printf("Amount deposited:");
    scanf("%d",&b);
    c=a+b;
    printf("Balance:%d\n",c);
    d=c-e;
    printf("Amount withdraw:");
    scanf("%d",&e);
    f=c-e;
    printf("Current balance:%d\n",f);
    return 0;
}
