

#include<stdio.h>
int main()
{
    int a[2][2]={1,2,3,4};
    
    int c[2][2]={0,0,0,0};
    int i,j;
    
    for(i=0;i<2;i++)
    
    {
        for(j=0;j<2;j++)
        {
         
            
            
        c[j][i]=a[i][j];
    
    }
    }
    for(i=0;i<2;i++)
    {
        for(j=0;j<2;j++)
    {
        printf("%d\t",c[i][j]);
    } 
        printf("\n");
    }    
       return 0;
    
    }


1	3	
2	4	
