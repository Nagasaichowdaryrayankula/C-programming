#include <stdio.h>
int main()
{
    int n,r,temp,sum=0;
    printf("Enter a num:");
    scanf("%d",&n);
    temp=n;
    while(n>0)
    {
        r=n%10;
        sum=sum+(r*r*r);
        n/=10;
    }
    if(temp==sum)
    {
        printf("Armstrong number");
    }
    else
    {
        printf("Not an Armstrong number");
    }
    return 0;
    
}
Enter a num:723
Not an Armstrong number
